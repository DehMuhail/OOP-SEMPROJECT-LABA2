30         {\cf19 if} (surface == NULL) \{\par
31             printf({\cf22 "Failed to render text: %s"}, SDL_GetError());\par
32         \}\par
33         position.w = surface->w;\par
34         position.h = surface->h;\par
35         texture = SDL_CreateTextureFromSurface(renderer, surface);\par
36         SDL_FreeSurface(surface);\par
37     \}\par
38 \par
39     {\cf18 void} render() \{\par
40         SDL_RenderCopy(renderer, texture, NULL, &position);\par
41     \}\par
